<h1>Listing friends</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Twitter</th>
    <th>Prefecture</th>
    <th></th>
  </tr>

<% @friends.each do |friend| %>
  <tr>
    <td><%= friend.name %></td>
    <td><%= friend.twitter %></td>
    <td><%= friend.prefecture %></td>
    <td><%= link_to 'Show', friend %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Friend', new_friend_path %>
